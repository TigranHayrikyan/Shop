<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:shape="rectangle"
    tools:ignore="NewApi">
    <corners android:radius="20dp" />
    <gradient
        android:type="linear"
        android:angle="200"
        android:startColor="#272B33"
        android:endColor="#14171F" />

    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"/>
    <stroke
        android:width="1dp"
        android:color="@color/black" />
</shape>

